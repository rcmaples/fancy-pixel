{
  "name": "sanity-plugin-fancy-pixel",
  "version": "1.0.0",
  "description": "Your very own nyan cat.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/rcmaples/fancy-pixel#readme",
  "bugs": {
    "url": "https://github.com/rcmaples/fancy-pixel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rcmaples/fancy-pixel.git"
  },
  "license": "MIT",
  "author": "RC Maples <rc@rcmaples.io>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "sanityPlugin": {
    "linkWatch": {
      "command": "npm run link-watch",
      "extensions": "ts,tsx,js,jsx,png,svg,gif,jpeg,css"
    }
  },
  "dependencies": {
    "@sanity/browserslist-config": "^1.0.3",
    "@sanity/icons": "^3.4.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/plugin-kit": "^4.0.18",
    "@sanity/ui": "^2.8.22",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.11.10",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.3",
    "sass": "^1.80.6",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "sanity": "^3.63.0"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "extends @sanity/browserslist-config"
  ],
  "verifyPackage": {
    "packageName": true,
    "module": true,
    "tsconfig": true,
    "tsc": true,
    "dependencies": true,
    "rollupConfig": true,
    "babelConfig": true,
    "sanityV2Json": true,
    "eslintImports": true,
    "scripts": true,
    "pkg-utils": true,
    "nodeEngine": true
  }
}
